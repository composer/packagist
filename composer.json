{
    "description": "Package Repository Website",
    "keywords": ["package", "composer"],
    "homepage": "https://packagist.org/",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Nils Adermann",
            "email": "naderman@naderman.de",
            "homepage": "https://www.naderman.de"
        },
        {
            "name": "Jordi Boggiano",
            "email": "j.boggiano@seld.be",
            "homepage": "https://seld.be"
        }
    ],
    "support": {
          "email": "contact@packagist.org"
    },
    "require": {
        "php": "^8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "algolia/algoliasearch-client-php": "^3.0",
        "babdev/pagerfanta-bundle": "^3",
        "beelab/recaptcha2-bundle": "^2.3",
        "cebe/markdown": "^1.1",
        "composer/composer": "^2.5@dev",
        "composer/pcre": "^3.0",
        "doctrine/annotations": "^1.0",
        "doctrine/common": "^3",
        "doctrine/doctrine-bundle": "^2.2",
        "doctrine/orm": "^2.7",
        "endroid/qr-code": "^4",
        "ezyang/htmlpurifier": "^4.6",
        "graze/dog-statsd": "^1",
        "knplabs/knp-menu-bundle": "^3",
        "knpuniversity/oauth2-client-bundle": "^2.8",
        "laminas/laminas-diagnostics": "^1.6",
        "laminas/laminas-feed": "^2.13",
        "league/oauth2-github": "^3.0",
        "nelmio/cors-bundle": "^2",
        "nelmio/security-bundle": "^3",
        "pagerfanta/core": "^3.1",
        "pagerfanta/doctrine-orm-adapter": "^3.1",
        "pagerfanta/twig": "^3.1",
        "phpdocumentor/reflection-docblock": "^5.2",
        "predis/predis": "^1.0",
        "scheb/2fa-backup-code": "^6",
        "scheb/2fa-bundle": "^6",
        "scheb/2fa-totp": "^6",
        "scheb/2fa-trusted-device": "^6",
        "seld/signal-handler": "^2",
        "sensio/framework-extra-bundle": "^6.1",
        "snc/redis-bundle": "^4.2",
        "symfony/asset": "^6",
        "symfony/console": "^6",
        "symfony/dotenv": "^6",
        "symfony/expression-language": "^6",
        "symfony/flex": "^2",
        "symfony/form": "^6",
        "symfony/framework-bundle": "^6",
        "symfony/http-client": "^6",
        "symfony/intl": "^6",
        "symfony/lock": "^6",
        "symfony/mailer": "^6",
        "symfony/monolog-bundle": "^3.6",
        "symfony/process": "^6",
        "symfony/property-access": "^6",
        "symfony/property-info": "^6",
        "symfony/proxy-manager-bridge": "^6",
        "symfony/runtime": "^6",
        "symfony/security-bundle": "^6",
        "symfony/serializer": "^6",
        "symfony/translation": "^6",
        "symfony/twig-bundle": "^6",
        "symfony/validator": "^6",
        "symfony/web-link": "^6",
        "symfony/yaml": "^6",
        "symfonycasts/verify-email-bundle": "^1.4",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/string-extra": "^3.2",
        "twig/twig": "^2.12|^3.0"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "phpstan/phpstan": "^1.3",
        "phpstan/phpstan-doctrine": "^1",
        "phpstan/phpstan-symfony": "^1.1",
        "phpunit/phpunit": "^9.5",
        "staabm/phpstan-dba": "^0.2",
        "symfony/browser-kit": "^6",
        "symfony/css-selector": "^6",
        "symfony/debug-bundle": "^6",
        "symfony/maker-bundle": "^1.30",
        "symfony/phpunit-bridge": "^6",
        "symfony/stopwatch": "^6",
        "symfony/var-dumper": "^6",
        "symfony/web-profiler-bundle": "^6"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "platform": {
            "php": "8.1.9"
        },
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "test": "bin/phpunit",
        "phpstan": "vendor/bin/phpstan analyse"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^6.1"
        },
        "symfony-bin-dir": "bin",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "config/parameters.yaml",
            "keep-outdated": true
        },
        "public-dir": "web/"
    }
}
